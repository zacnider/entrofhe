{
  "buildCommand": "bash install-examples-deps.sh && cd frontend && npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "cd frontend && npm install --legacy-peer-deps",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/proxy?path=:path*"
    },
    {
      "source": "/api/indexer/:path*",
      "destination": "/api/indexer-proxy?path=:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/indexer/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "functions": {
    "api/proxy.ts": {
      "maxDuration": 300
    },
    "api/indexer-proxy.ts": {
      "maxDuration": 300
    }
  }
}